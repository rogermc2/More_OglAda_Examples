
with Magick_Type;

package Profile is

   type Pixel_Packet is record
      Red     : Magick_Type.Quantum;
      Green   : Magick_Type.Quantum;
      Blue    : Magick_Type.Quantum;
      Opacity : Magick_Type.Quantum;
   end record;


   type Quantum_Pixel_Packet is record
      Red     : Magick_Type.Quantum;
      Green   : Magick_Type.Quantum;
      Blue    : Magick_Type.Quantum;
      Opacity : Magick_Type.Quantum;
      Index   : Magick_Type.Quantum;
   end record;

end Magick_Pixel;
